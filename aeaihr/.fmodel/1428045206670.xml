<stan:StandardFuncModel xmlns:stan="http://www.hotweb.agileai.com/model/standard"><stan:BaseInfo detailJspName="leave/HrLeaveManageEdit.jsp" detailTitle="请假申请" template="Default" listJspName="leave/HrLeaveManageList.jsp" listTitle="请假申请" tableName="hr_leave" pkGenPolicy="chargen"><stan:Service serviceId="hrLeaveManageService" ImplClassName="com.agileai.hr.bizmoduler.leave.HrLeaveManageImpl" InterfaceName="com.agileai.hr.bizmoduler.leave.HrLeaveManage"/><stan:Handler editHandlerId="HrLeaveManageEdit" editHandlerClass="com.agileai.hr.controller.leave.HrLeaveManageEditHandler" listHandlerId="HrLeaveManageList" listHandlerClass="com.agileai.hr.controller.leave.HrLeaveManageListHandler"/><stan:SqlResource sqlNameSpace="hr_leave" sqlMapPath="sqlmap/hr_leave.xml"/><stan:QueryListSql>select a.LEA_ID,a.USER_ID,b.USER_NAME as USER_ID_NAME,a.LEA_DATE,a.LEA_TYPE,a.LEA_CAUSE,a.LEA_APPOVER,c.USER_NAME as LEA_APPOVER_NAME,a.LEA_APP_TIME,a.LEA_APP_OPINION,a.APP_RESULT,a.LEA_SDATE,a.LEA_EDATE,a.LEA_DAYS,STATE
